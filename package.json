{
  "name": "pattern",
  "version": "0.1.0",
  "description": "Pattern matching server",
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "test-only": "nyc mocha",
    "test": "yarn run lint && yarn run test-only",
    "lint": "tslint -p tslint.json",
    "prepublish": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AugurWorks/pattern.git"
  },
  "author": "TheConnMan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AugurWorks/pattern/issues"
  },
  "homepage": "https://github.com/AugurWorks/pattern#readme",
  "dependencies": {
    "express": "^4.16.3",
    "log4js": "^3.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/log4js": "^2.3.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "@types/sinon": "^5.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "sinon": "^6.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "nyc": {
    "include": [
      "ts/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
